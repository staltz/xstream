{
  "compilerOptions": {
    "removeComments": false,
    "preserveConstEnums": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "declaration": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "module": "commonjs",
    "noEmitHelpers": false,
    "target": "ES5",
    "outDir": "./",
    "lib": [
      "dom",
      "es5",
      "es2015.collection",
      "es2015.promise"
    ]
  },
  "formatCodeOptions": {
    "indentSize": 2,
    "tabSize": 2
  },
  "files": [
    "src/extra/buffer.ts",
    "src/extra/concat.ts",
    "src/extra/debounce.ts",
    "src/extra/delay.ts",
    "src/extra/dropRepeats.ts",
    "src/extra/dropUntil.ts",
    "src/extra/flattenSequentially.ts",
    "src/extra/flattenConcurrently.ts",
    "src/extra/fromDiagram.ts",
    "src/extra/fromEvent.ts",
    "src/extra/pairwise.ts",
    "src/extra/sampleCombine.ts",
    "src/extra/split.ts",
    "src/extra/throttle.ts",
    "src/extra/tween.ts",
    "src/index.ts"
  ],
  "filesGlob": [
    "typings/index.d.ts",
    "src/**/*.ts"
  ],
  "atom": {
    "rewriteTsconfig": true
  }
}
